    rdfs:domain etutor:Goal ;
    rdfs:range  xsd:dateTime .

etutor:hasOwner
    rdf:type    rdf:Property ;
    rdfs:domain etutor:Goal ;
    rdfs:range  xsd:string .

etutor:hasReferenceCnt
