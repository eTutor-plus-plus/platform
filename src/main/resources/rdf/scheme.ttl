@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix etutor: <http://www.dke.uni-linz.ac.at/etutorpp/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

## Learning goals
# Classes
etutor:Goal rdf:type rdfs:Class .
etutor:SubGoal rdfs:subClassOf etutor:Goal .

# Properties
etutor:hasSubGoal rdf:type rdfs:Property ;
				  rdfs:domain etutor:Goal ;
				  rdfs:range etutor:Goal .

etutor:dependsOn rdf:type rdfs:Property ;
				 rdfs:domain etutor:Goal ;
				 rdfs:range etutor:Goal .

etutor:hasDescription rdf:type rdfs:Property ;
					  rdfs:domain etutor:Goal ;
					  rdfs:range xsd:string .

etutor:isPrivate rdf:type rdfs:Property ;
				 rdfs:domain etutor:Goal ;
				 rdfs:range xsd:boolean .

etutor:hasChangeDate rdf:type rdfs:Property ;
					 rdfs:domain etutor:Goal ;
					 rdfs:range xsd:dateTime .

etutor:hasOwner rdf:type rdfs:Property ;
				rdfs:domain etutor:Goal ;
				rdfs:range xsd:string .

etutor:hasReferenceCnt rdf:type rdfs:Property ;
                       rdfs:domain etutor:Goal ;
                       rdfs:range xsd:int .

etutor:hasRootGoal rdf:type rdfs:Property ;
                   rdfs:domain etutor:Goal ;
                   rdfs:range etutor:Goal .

## Coureses
# Classes
etutor:Course rdf:type rdfs:Class .

# Properties
etutor:hasCourseDescription rdf:type rdfs:Property ;
                            rdfs:domain etutor:Course ;
                            rdfs:range xsd:string .

etutor:hasCourseName rdf:type rdfs:Property ;
                     rdfs:domain etutor:Course ;
                     rdfs:range xsd:string .

etutor:hasCourseLink rdf:type rdfs:Property ;
                     rdfs:domain etutor:Course ;
                     rdfs:range xsd:anyURI .

etutor:hasCourseType rdf:type rdfs:Property ;
                     rdfs:domain etutor:Course ;
                     rdfs:range xsd:string .

etutor:hasCourseCreator rdf:type rdfs:Property ;
                        rdfs:domain etutor:Course ;
                        rdfs:range xsd:string .

etutor:hasGoal rdf:type rdfs:Property ;
               rdfs:domain etutor:Course ;
               rdfs:range etutor:Goal .
